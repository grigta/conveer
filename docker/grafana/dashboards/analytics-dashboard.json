{
  "dashboard": {
    "id": null,
    "uid": "analytics-service",
    "title": "Analytics Service Dashboard",
    "tags": ["analytics", "monitoring", "conveer"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "title": "Total Accounts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(analytics_total_accounts)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "title": "Overall Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(analytics_success_rate)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 75}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "title": "Overall Ban Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(analytics_ban_rate)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 15}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "title": "Active Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(analytics_active_alerts)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "title": "Accounts Trend (7 days)",
        "type": "graph",
        "targets": [
          {
            "expr": "analytics_total_accounts{platform=\"all\"}",
            "legendFormat": "Total",
            "refId": "A"
          },
          {
            "expr": "analytics_total_accounts{platform=\"vk\"}",
            "legendFormat": "VK",
            "refId": "B"
          },
          {
            "expr": "analytics_total_accounts{platform=\"telegram\"}",
            "legendFormat": "Telegram",
            "refId": "C"
          },
          {
            "expr": "analytics_total_accounts{platform=\"mail\"}",
            "legendFormat": "Mail",
            "refId": "D"
          },
          {
            "expr": "analytics_total_accounts{platform=\"max\"}",
            "legendFormat": "Max",
            "refId": "E"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "show": true
          },
          {
            "format": "short",
            "show": false
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "title": "Expenses Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "analytics_total_spent{type=\"total\"}",
            "legendFormat": "Total",
            "refId": "A"
          },
          {
            "expr": "analytics_total_spent{type=\"sms\"}",
            "legendFormat": "SMS",
            "refId": "B"
          },
          {
            "expr": "analytics_total_spent{type=\"proxy\"}",
            "legendFormat": "Proxy",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "currencyUSD",
            "show": true
          },
          {
            "format": "short",
            "show": false
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 12},
        "title": "Ban Rate by Platform",
        "type": "bargauge",
        "targets": [
          {
            "expr": "analytics_ban_rate",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 30,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 15}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 12},
        "title": "Success Rate by Platform",
        "type": "bargauge",
        "targets": [
          {
            "expr": "analytics_success_rate",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 75}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 12},
        "title": "Error Rate by Platform",
        "type": "bargauge",
        "targets": [
          {
            "expr": "analytics_error_rate",
            "legendFormat": "{{platform}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 30,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18},
        "title": "Service Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(analytics_aggregation_duration_seconds_sum[5m]) / rate(analytics_aggregation_duration_seconds_count[5m])",
            "legendFormat": "Aggregation Duration",
            "refId": "A"
          },
          {
            "expr": "rate(analytics_forecast_generation_duration_seconds_sum[5m]) / rate(analytics_forecast_generation_duration_seconds_count[5m])",
            "legendFormat": "Forecast Generation",
            "refId": "B"
          },
          {
            "expr": "rate(analytics_alert_check_duration_seconds_sum[5m]) / rate(analytics_alert_check_duration_seconds_count[5m])",
            "legendFormat": "Alert Check",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "show": true
          },
          {
            "format": "short",
            "show": false
          }
        ]
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18},
        "title": "Alerts Fired",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(analytics_alerts_fired_total[1h])",
            "legendFormat": "{{severity}} - {{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "show": true
          },
          {
            "format": "short",
            "show": false
          }
        ]
      },
      {
        "id": 12,
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "title": "Aggregation Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(analytics_aggregation_successes_total[1h]) / (rate(analytics_aggregation_successes_total[1h]) + rate(analytics_aggregation_errors_total[1h])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "title": "Forecasts Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(analytics_forecasts_generated_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 14,
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "title": "Recommendations Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(analytics_recommendations_generated_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(analytics_cache_hits_total[1h]) / (rate(analytics_cache_hits_total[1h]) + rate(analytics_cache_misses_total[1h])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}